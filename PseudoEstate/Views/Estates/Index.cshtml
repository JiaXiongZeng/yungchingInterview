            <th>
                @Html.DisplayNameFor(model => model.ModifyDtm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteDtm)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EstateId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
